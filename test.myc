struct Line {
	:length uint8;
	:pixel  uint8;
}

function print_line(:line Line) bool {
	if line.length < [uint8]0 {
		return true;
	}

	for (:i uint8 = [uint8]1; i < line.length; i = i + [uint8]1) {
		printf("%c", line.pixel);
	}
	printf("\n");
	
	false
}



