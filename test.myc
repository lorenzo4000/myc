function print_line(:length int64, :x bool) {
		while length > 0 {
			printf("%c", length + 42);
			length = length - 1;
		}
		printf("\n");
}

function main() {
	:CAP int64 = 30;
	:INCR int64 = 2;
	
	:x int64 = 0;
	:sing int64 = 1;

	while true {
 		if sing > 0 {
			x = x + INCR;
		} else {
			x = x - INCR;
		}
		if x >= CAP {
			sing = 0;
		}
		if x <= 0 {
			sing = 1;
		}
		print_line(x, true);
	}
}
